# Environment variables for AHGD ETL Pipeline
# Copy this file to .env and update values as needed

# Base directory for all file operations
BASE_DIR=/path/to/your/project

# Processing options
FORCE_DOWNLOAD=false
STOP_ON_ERROR=true
LOG_LEVEL=INFO

# Time dimension configuration
TIME_DIM_START_YEAR=2010
TIME_DIM_END_YEAR=2030

# Optional: Snowflake configuration path
SNOWFLAKE_CONFIG=/path/to/snowflake/config.json